<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StyledButton">
        <attr name="accent" format="enum">
            <enum name="teal" value="0" />
            <enum name="orange" value="1" />
            <enum name="red" value="2" />
            <enum name="blue" value="3" />
            <enum name="yellow" value="4" />
        </attr>
        <attr name="type" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
            <enum name="tertiary" value="2" />
            <enum name="text" value="3" />
        </attr>
    </declare-styleable>

</resources>